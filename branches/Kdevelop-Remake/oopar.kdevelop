        <ccompiler>kdevgccoptions</ccompiler>
        <cxxcompiler>kdevgppoptions</cxxcompiler>
        <f77compiler>kdevg77options</f77compiler>
        <cxxflags>-no-inline -O0 -g3</cxxflags>
        <envvars/>
        <topsourcedir/>
        <cppflags>-DMPICH_IGNORE_CXX_SEEK -DBLOCKING -DMTSEND</cppflags>
        <ldflags/>
        <ccompilerbinary>mpicc</ccompilerbinary>
        <cxxcompilerbinary>mpicxx</cxxcompilerbinary>
        <f77compilerbinary>g77</f77compilerbinary>
        <cflags/>
        <f77flags/>
      </debug>
      <default>
        <envvars/>
        <configargs>--prefix=/home/cluster/proj/proj076/ecrylo/installDIR/std6 --enable-mpilam=yes --with-mpilam-dir=/home/cluster/proj/proj076/ecrylo/installDIR/std6 --with-pz-dir=/home/cluster/proj/proj076/ecrylo/installDIR/std6 --with-log4cxx-dir=/home/cluster/proj/proj076/ecrylo/installDIR/std6</configargs>
        <builddir/>
        <topsourcedir/>
        <cppflags>-m64</cppflags>
        <ldflags/>
        <ccompiler>kdevgccoptions</ccompiler>
        <cxxcompiler>kdevgppoptions</cxxcompiler>
        <f77compiler>kdevpgf77options</f77compiler>
        <ccompilerbinary/>
        <cxxcompilerbinary>mpic++</cxxcompilerbinary>
        <f77compilerbinary/>
        <cflags/>
        <cxxflags/>
        <f77flags/>
      </default>
      <debug_CFD>
        <envvars/>
        <configargs>--prefix=/home/cluster/proj/proj076/fortiago/Embraer/installDIR --enable-debug=full --with-mpi-dir=/home/cluster/proj/proj076/ecrylo/installDIR/std5/ --with-pz-dir=/home/cluster/proj/proj076/fortiago/Embraer/installDIR --enable-mpi=yes</configargs>
        <builddir>debug_CFD</builddir>
        <topsourcedir/>
        <cppflags>-m64</cppflags>
        <ldflags/>
        <ccompiler>kdevgccoptions</ccompiler>
        <cxxcompiler>kdevgppoptions</cxxcompiler>
        <f77compiler>kdevpgf77options</f77compiler>
        <ccompilerbinary>/usr/local/gcc-3.3.5/installdir/bin/gcc</ccompilerbinary>
        <cxxcompilerbinary>/usr/local/gcc-3.3.5/installdir/bin/g++</cxxcompilerbinary>
        <f77compilerbinary/>
        <cflags/>
        <cxxflags/>
        <f77flags/>
      </debug_CFD>
      <debug_mpe_logged>
        <envvars/>
        <configargs>--enable-debug=full --enable-mpi=yes --enable-mpe=yes --with-mpi-dir=$HOME/Projetos/MPIDebug --enable-pz=yes --with-pz-dir=$HOME/Projetos/PZInstall --prefix=$HOME/Projetos/OOParInstall --enable-log4cxx=yes</configargs>
        <builddir>debug_mpe_logged</builddir>
        <topsourcedir/>
        <cppflags>-DMPICH_IGNORE_CXX_SEEK -DBLOCKING -DMTSEND</cppflags>
        <ldflags/>
        <ccompiler>kdevgccoptions</ccompiler>
        <cxxcompiler>kdevgppoptions</cxxcompiler>
        <f77compiler>kdevpgf77options</f77compiler>
        <ccompilerbinary>$HOME/Projetos/MPIDebug/bin/mpicc</ccompilerbinary>
        <cxxcompilerbinary>$HOME/Projetos/MPIDebug/bin/mpicxx</cxxcompilerbinary>
        <f77compilerbinary/>
        <cflags/>
        <cxxflags>-no-inline -O0 -g3</cxxflags>
        <f77flags/>
      </debug_mpe_logged>
      <debug_mpe>
        <envvars/>
        <configargs>--enable-debug=full --enable-mpi=yes --enable-mpe=yes --with-mpi-dir=$HOME/Projetos/MPIDebug --enable-pz=yes --with-pz-dir=$HOME/Projetos/PZInstall --prefix=$HOME/Projetos/OOParInstall --enable-log4cxx=no</configargs>
        <builddir>debug_mpe</builddir>
        <topsourcedir/>
        <cppflags/>
        <ldflags/>
        <ccompiler>kdevgccoptions</ccompiler>
        <cxxcompiler>kdevgppoptions</cxxcompiler>
        <f77compiler>kdevpgf77options</f77compiler>
