  <kdevautoproject>
    <general>
      <activetarget>Projects/TestTask/main</activetarget>
      <useconfiguration>debug_mpe</useconfiguration>
    </general>
    <run>
      <mainprogram>/home/longhin/Projetos/OOPar/debug/Projects/TestTask/main</mainprogram>
      <directoryradio>executable</directoryradio>
      <customdirectory>/</customdirectory>
      <programargs>-np 4 ./main</programargs>
      <terminal>false</terminal>
      <autocompile>true</autocompile>
      <envvars/>
      <globaldebugarguments/>
