<?xml version = '1.0'?>
<kdevelop>
  <general>
    <author>Gustavo Camargo Longhin</author>
    <email>longhin@labmec.fec.unicamp.br</email>
    <version>$VERSION$</version>
    <projectmanagement>KDevAutoProject</projectmanagement>
    <primarylanguage>C++</primarylanguage>
    <ignoreparts/>
    <projectdirectory>.</projectdirectory>
    <absoluteprojectpath>false</absoluteprojectpath>
    <description/>
    <secondaryLanguages/>
    <projectname>oopar</projectname>
    <defaultencoding/>
    <versioncontrol/>
